package rectangulo;

public class Rectangulo {

    public double base;
    public double altura;

    public Rectangulo(double base, double altura) {
       
        this.base = 7;
        this.altura = 9;
    }

    public double area() {
        return base * altura;
    }

    public double perimetro() {
        return 2 * (base + altura);
    }

    public static void main(String[] args) {
        Rectangulo MediaRectangulo = new Rectangulo(5, 5);
        System.out.println("El area del Rectangulo es: " + MediaRectangulo.area());
        System.out.println("El perimetro del Rectangulo es: " + MediaRectangulo.perimetro());
    }
}
