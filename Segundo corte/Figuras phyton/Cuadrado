class Cuadrado:

    def __init__(self, base):
        self.base = base

    def area(self):
        return self.base **2

    def perimetro(self):
        return   (self.base *4)

MediaCuadrado = Cuadrado(5)
print("El area del cuadrado es:", MediaCuadrado.area())
print("El perÃ­metro del cuadrado es:", MediaCuadrado.perimetro())
