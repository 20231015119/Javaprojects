class Rectangulo:

    def __init__(self, base, altura):
        self.base = base
        self.altura = altura

    def area(self):
        return self.base * self.altura

    def perimetro(self):
        return 2 * (self.base + self.altura)

MediaRectangulo = Rectangulo(7, 9) 
print("El área del rectángulo es:", MediaRectangulo.area())
print("El perímetro del rectángulo es:", MediaRectangulo.perimetro())
